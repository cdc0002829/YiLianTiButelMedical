<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" 
    android:background="@color/color_white">
     <com.butel.connectevent.test.utils.MyEditText 
          android:id="@+id/v_edit_result"
            android:layout_width="fill_parent"
            android:layout_height="120dp"
            android:layout_alignParentLeft="true"
            android:editable="false"
            android:gravity="left"
            android:hint="EventConnect-Show"
            android:isScrollContainer="true"
            android:scrollbars="vertical"
            android:singleLine="false"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp"
            android:background="#e0e0e0"
         /> 
            <Button
            android:id="@+id/v_btn_clear"
            android:text="清  屏"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            ></Button>
               <Button
            android:id="@+id/v_btn_close"
            android:layout_below="@id/v_btn_clear"
            android:text="关  闭"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            ></Button>
               <ScrollView 
                android:id="@+id/v_sv_ui"
                android:layout_below="@id/v_edit_result"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                >
                <RelativeLayout 
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    >
                   <LinearLayout 
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:id="@+id/v_ll_senior"
                             >
                         <Button 
                            android:id="@+id/v_btn_createAdvCall"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="createADVCall"
                            android:visibility="gone"/>
                        <EditText 
                            android:id="@+id/v_edt_ADVCallUser"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:hint=" ADVCallUser多条逗号分割"/>
                        <EditText 
                            android:id="@+id/v_edt_ADVCallUserInfo"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:hint="extendInfo"/>
                         <Button 
                            android:id="@+id/v_btn_addMember"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="addMember"/>
                         <EditText 
                             android:layout_width="fill_parent"
                             android:layout_height="wrap_content"
                             android:hint="adv_dest_nube"
                             android:id="@+id/v_edt_adv_dest"
                             />
                         <Button 
                            android:id="@+id/v_btn_hold"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="hold"/>
                         <Button 
                            android:id="@+id/v_btn_resume"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="resume"/>
                         <EditText 
                             android:id="@+id/v_edt_monitorkey"
                             android:layout_width="fill_parent"
                             android:layout_height="wrap_content"
                             android:text="monitorkey"/>
                            <Button 
                            android:id="@+id/v_btn_adv_monitor"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="monitor"/>
                            <LinearLayout 
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <CheckBox 
                                    android:id="@+id/v_ch_upaudio"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="上行音频"/>
                                <CheckBox 
                                    android:id="@+id/v_ch_upvideo"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="上行视频"/>
                            </LinearLayout>
                              <LinearLayout 
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <CheckBox 
                                    android:id="@+id/v_ch_downaudio"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="下行音频"/>
                                <CheckBox 
                                    android:id="@+id/v_ch_downvideo"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="下行视频"/>
                            </LinearLayout>
                             <Button 
                            android:id="@+id/v_btn_EnableMedia"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="EnableMedia"/>
                            <Button 
                            android:id="@+id/v_btn_advRedirectCall"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="AdvRedirectCall"/>
                            <Button 
                            android:id="@+id/v_btn_RemoveMember"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="RemoveMember"/>
                            <Button 
                            android:id="@+id/v_btn_GetCallInfo"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="GetCallInfo"/>
                            <CheckBox 
                                android:id="@+id/v_ch_isforce"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:text="是否指定isforce"/>
                            <EditText 
                                android:id="@+id/v_edt_videoSrcNumber"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:hint="视/音频源用户号码"/>
                            <Button 
                                android:id="@+id/v_btn_SetRemoteVideo"
                                android:text="SetRemoteVideo"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"/>
                               <Button 
                                android:id="@+id/v_btn_SetRemoteAudio"
                                android:text="SetRemoteAudio"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"/>
                            <Button 
                                android:id="@+id/v_btn_SetDefaultVideo"
                                android:text="SetDefaultVideo"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"/>
                            
                            
                            
                            
                            <TextView
                        android:id="@+id/txt_ismonitor"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="是否使用monitor"/>
                        <EditText
                        android:id="@+id/v_edt_ismonitor"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:numeric="integer"
                        android:text="0" />
                   <EditText
                        android:id="@+id/v_edt_monitorCode"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:hint="" />
                   <Button
                        android:id="@+id/v_btn_ButelSetMonitorCode"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="ButelSetMonitorCode" />
                             <EditText 
                                 android:id="@+id/v_edt_dest"
                                 android:hint="destnube"
                                 android:layout_width="fill_parent"
                                 android:layout_height="wrap_content"
                                 android:visibility="gone"/>
                              <EditText 
                                 android:id="@+id/v_edt_cad"
                                 android:hint="CAD"
                                 android:layout_width="fill_parent"
                                 android:layout_height="wrap_content"
                                 android:visibility="gone"/>
                             
                             <Button 
                                android:id="@+id/v_btn_makecallS"
                                android:text="makecallAdv"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:visibility="gone"
                                />
                             <Button 
                                android:id="@+id/v_btn_makecallDetect"
                                android:text="makecallDetect"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:visibility="gone"/>
                        </LinearLayout>
                    
                </RelativeLayout>
                </ScrollView>
                
</RelativeLayout>
