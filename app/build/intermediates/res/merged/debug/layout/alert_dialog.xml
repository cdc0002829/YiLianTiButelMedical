<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f9fb"
    android:orientation="vertical">


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="#e3e4e5" />


    <TextView
        android:id="@+id/delete_zhuanfa_tv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y200"
        android:layout_margin="@dimen/x4"
        android:gravity="center"
        android:textColor="#323233"
        android:textSize="@dimen/x36" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="#d7d9db" />

    <TextView
        android:id="@+id/cancle_zhuanfa_tv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:layout_margin="@dimen/x4"
        android:layout_marginTop="@dimen/y10"
        android:gravity="center"
        android:text="@string/iknow"
        android:textColor="#323233"
        android:textSize="@dimen/x36" />

</LinearLayout>