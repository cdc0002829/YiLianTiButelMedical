<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y160"
    android:id="@+id/imag_url"
    android:orientation="horizontal"
    android:background="#ffffff">
        <cn.redcdn.hvs.im.view.RoundImageView
            android:scaleType="centerCrop"
            android:layout_marginLeft="@dimen/x28"
            android:id="@+id/btn_head"
            android:layout_width="@dimen/x128"
            android:layout_height="@dimen/y128"
            android:background="@drawable/doctor_default"
            android:layout_gravity="center"
            android:contentDescription="@null"
            android:visibility="visible"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nub_num"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/x32"
            android:layout_marginLeft="@dimen/x34"
            android:textColor="#000000"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:id="@+id/video_number"
            android:layout_marginLeft="@dimen/x22"
            android:textSize="@dimen/x32"
            android:textColor="#000000"/>
</LinearLayout>