<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <include
        android:id="@+id/titlebar1"
        layout="@layout/titlebar"></include>

    <ImageView
        android:id="@+id/certification_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/titlebar1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y112"
        android:src="@drawable/certify" />

    <TextView
        android:id="@+id/passed_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/certification_iv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y88"
        android:text="@string/has_passed"
        android:textColor="@color/qn_dialog_body_color"
        android:textSize="@dimen/x36" />

    <TextView
        android:id="@+id/pass_subscibe_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/passed_tv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y40"
        android:text="@string/passed_subscribe"
        android:textColor="#666666"
        android:textSize="@dimen/x28" />

    <TextView
        android:id="@+id/modify_tv"
        android:clickable="true"
        android:textSize="@dimen/x36"
        android:textColor="@color/recording_video_title_presscolor"
        android:text="@string/please_resubmit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y72" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/x260"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/y30"
            android:autoLink="phone"
            android:gravity="center"
            android:text="@string/audit_telephone"
            android:textColor="#b5b5b5"
            android:textSize="@dimen/x24" />

        <TextView
            android:id="@+id/auding_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/y30"
            android:gravity="center"
            android:textColor="#b5b5b5"
            android:textSize="@dimen/x24" />
    </LinearLayout>

    <ImageView
        android:maxHeight="@dimen/y200"
        android:adjustViewBounds="true"
        android:layout_marginTop="47dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/image"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:visibility="visible"
        android:layout_below="@+id/pass_subscibe_tv"
        android:layout_centerHorizontal="true" />

</RelativeLayout>