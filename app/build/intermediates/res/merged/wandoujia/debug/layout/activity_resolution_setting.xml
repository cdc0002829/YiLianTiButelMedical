<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/systembg"
    android:orientation="vertical" >

    <RelativeLayout style="@style/layout_head" >

        <Button
            android:id="@+id/setting_resolution_back"
            style="@style/back_btn" />

        <TextView
            style="@style/middle_text"
            android:text="@string/camera_setting" />

        <View
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y1"
            android:background="@color/color_butel_selfinfo_darkline_color" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_set_resolution"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="visible"
        >

        <LinearLayout
            android:id="@+id/set_front_camare"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x30"
                android:background="#ebeef2"
                android:gravity="center|left"
                android:text="@string/camera_front"
                android:textColor="@color/setting_title_txt"
                android:textSize="@dimen/x24" />

            <RelativeLayout
                android:id="@+id/set_front_camare_resolution_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y120"
                android:background="@drawable/contact_item_selector"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x30"
                    android:text="@string/resolution"
                    android:textColor="@color/setting_item_txt"
                    android:textSize="@dimen/x29" />

                <TextView
                    android:id="@+id/set_front_camare_resolution_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:layout_toLeftOf="@+id/set_front_camare_resolution_btn"
                    android:text="640X360"
                    android:textColor="#969799"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/set_front_camare_resolution_btn"
                    android:layout_width="@dimen/x17"
                    android:layout_height="@dimen/y26"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/nextlittle" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:background="#e0e0e0" />

            <RelativeLayout
                android:id="@+id/set_front_camare_rate_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y120"
                android:background="@drawable/contact_item_selector"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x30"
                    android:text="@string/rate"
                    android:textColor="@color/setting_item_txt"
                    android:textSize="@dimen/x29" />

                <TextView
                    android:id="@+id/set_front_camare_rate_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:layout_toLeftOf="@+id/set_front_camare_rate_btn"
                    android:text="300kbps"
                    android:textColor="#969799"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/set_front_camare_rate_btn"
                    android:layout_width="@dimen/x17"
                    android:layout_height="@dimen/y26"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/nextlittle" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:background="#e0e0e0" />

            <RelativeLayout
                android:id="@+id/set_front_camare_frame_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y120"
                android:background="@drawable/contact_item_selector"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x30"
                    android:text="@string/frame"
                    android:textColor="@color/setting_item_txt"
                    android:textSize="@dimen/x29" />

                <TextView
                    android:id="@+id/set_front_camare_frame_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:layout_toLeftOf="@+id/set_front_camera_frame_rate"
                    android:text="15fps"
                    android:textColor="#969799"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/set_front_camera_frame_rate"
                    android:layout_width="@dimen/x17"
                    android:layout_height="@dimen/y26"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/nextlittle"
                     />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/set_back_camare"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/x44"
                android:layout_marginLeft="@dimen/x30"
                android:background="#ebeef2"
                android:gravity="center|left"
                android:text="@string/camera_back"
                android:textColor="@color/setting_title_txt"
                android:textSize="@dimen/x24" />

            <RelativeLayout
                android:id="@+id/set_back_camare_resolution_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y120"
                android:background="@drawable/contact_item_selector"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x30"
                    android:text="@string/resolution"
                    android:textColor="@color/setting_item_txt"
                    android:textSize="@dimen/x29" />

                <TextView
                    android:id="@+id/set_back_camare_resolution_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:layout_toLeftOf="@+id/set_back_camare_resolution_btn"
                    android:text="640X360"
                    android:textColor="#969799"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/set_back_camare_resolution_btn"
                    android:layout_width="@dimen/x17"
                    android:layout_height="@dimen/y26"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/nextlittle" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:background="#e0e0e0" />

            <RelativeLayout
                android:id="@+id/set_back_camare_rate_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y120"
                android:background="@drawable/contact_item_selector"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x30"
                    android:text="@string/rate"
                    android:textColor="@color/setting_item_txt"
                    android:textSize="@dimen/x29" />

                <TextView
                    android:id="@+id/set_back_camare_rate_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:layout_toLeftOf="@+id/set_back_camare_rate_btn"
                    android:text="300kbps"
                    android:textColor="#969799"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/set_back_camare_rate_btn"
                    android:layout_width="@dimen/x17"
                    android:layout_height="@dimen/y26"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/nextlittle" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:background="#e0e0e0" />

            <RelativeLayout
                android:id="@+id/set_back_camare_frame_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y120"
                android:background="@drawable/contact_item_selector"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x30"
                    android:text="@string/frame"
                    android:textColor="@color/setting_item_txt"
                    android:textSize="@dimen/x29" />

                <TextView
                    android:id="@+id/set_back_camare_frame_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:layout_toLeftOf="@+id/set_back_camera_frame_rate"
                    android:text="15fps"
                    android:textColor="#969799"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/set_back_camera_frame_rate"
                    android:layout_width="@dimen/x17"
                    android:layout_height="@dimen/y26"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/nextlittle"
                     />
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>