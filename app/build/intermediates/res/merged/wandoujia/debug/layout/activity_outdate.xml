<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/titlebar1"
        layout="@layout/titlebar"></include>

    <ImageView
        android:id="@+id/certification_iv"
        android:layout_width="@dimen/x150"
        android:layout_height="@dimen/y150"
        android:layout_below="@id/titlebar1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y230"
        android:src="@drawable/outdate" />

    <TextView
        android:id="@+id/passed_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/certification_iv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y88"
        android:text="@string/out_date"
        android:textColor="@color/text_color_name"
        android:textSize="@dimen/x36" />
</RelativeLayout>